  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "env": {
    "NEXT_PUBLIC_BUSINESS_NAME": "A Pretty Girl Matter",
    "NEXT_PUBLIC_BUSINESS_PHONE": "(919) 441-0932",
